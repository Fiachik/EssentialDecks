        <Transform>
            <Not/>
        </Transform>
        <BindProperty>visible</BindProperty>
    </Connection>
    <Children>
        <Spinny>
      <TooltipId>spinny_channel1</TooltipId>
            <Channel>1</Channel>
            <Pos>172,310</Pos>
            <Size>410,410</Size>
