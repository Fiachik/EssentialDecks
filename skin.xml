<!DOCTYPE skin>
<skin>
<manifest>
    <title>Basic Controller UI</title>
    <author>Fauchic</author>
    <version>0.7</version>
    <description>Basic 2-channel skin inspired by Pioneer-style controllers, made for easy mixing</description>
    <language>en</language>
    <license>Creative Commons Attribution, Share-Alike 3.0 Unported</license>
        <attributes>
        <attribute persist="true" config_key="[Skin],pad_mode_2">0</attribute>
            <attribute persist="true" config_key="[Skin],pad_mode">0</attribute>
            <attribute persist="true" config_key="[Skin],show_library">0</attribute>

        </attributes>
    </manifest>
 <Style src="skin:style.qss"/>


    <WidgetGroup>
        <Pos>0,0</Pos>
        <Size>1920f,1080f</Size>
        <MinimumSize>1024,768</MinimumSize>
        <Style>QGroupBox { background: url(skin:/Background.png); }</Style>
    </WidgetGroup>
    

<SetVariable name="[Skin],pad_mode">0</SetVariable>
